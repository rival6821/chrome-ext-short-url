{
    "manifest_version": 3,
    "name": "Create Short URL",
    "version": "1.0",
    "description": "Create Short URL",
    "action": {
      "default_popup" : "src/popup.html",
      "default_icon" : "image/icon.png"
    },
    "host_permissions" : [
      "https://url.ilhoon.kr/*"
    ],
    "icons" : {
      "16" : "image/icon-16.png",
      "32" : "image/icon-32.png",
      "128" : "image/icon-128.png"
    },
    "permissions": [
      "tabs",
      "activeTab",
      "clipboardWrite"
    ]
  }